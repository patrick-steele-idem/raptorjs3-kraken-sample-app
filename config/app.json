{
    
    "express": {
        "x-powered-by": false,
        "trust proxy": false,
        "jsonp callback name": null,
        "json replacer": null,
        "json spaces": 0,
        "case sensitive routing": false,
        "strict routing": false
    },

    "middleware": {
        "static": {
            "enabled": true,
            "priority": 20,
            "module": "express",
            "route": "/static",
            "arguments": [ "build" ]
        },
        "query": {
            "enabled": true,
            "module": "express",
            "priority": 10
        },
        "compress": {
            "enabled": true
        }
    },
    "raptor-optimizer": {
        "plugins": {
            "raptor-optimizer-require": {},
            "raptor-optimizer-rhtml": {},
            "raptor-optimizer-less": {}
        },
        "fileWriter": {
            "outputDir": "path:./build",
            "urlPrefix": "/static",
            "includeSlotNames": false,
            "checksumsEnabled": true
        },
        "enabledExtensions": ["browser"],
        "transforms": [
            "resolve-css-urls"
        ],
        "cache": {
            "DEFAULT": {
                "disk": {
                    "read": true,
                    "write": false,
                    "dir": "path:./work/raptor-optimizer"
                },
                "memory": {
                    "maxEntries": -1
                }
            }
        },
        "bundles": [
            {
                "name": "core",
                "dependencies": [
                    "raptor-modules/client"
                ]
            },
            {
                "name": "jquery",
                "dependencies": [
                    "require jquery"
                ]
            }
        ]
    },
    "raptor-dust": {
        "baseDir": "path:./src"
    }
}
